# Supabase (get from supabase.com dashboard)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Contract Info
NEXT_PUBLIC_CONTRACT_ADDRESS=your_solana_contract_address
NEXT_PUBLIC_CASHTAG=$ATTENTION

# Admin wallet - BOTH are required:
# Server-side (for API verification - KEEP SECRET)
ADMIN_WALLET=your_admin_wallet_address
# Client-side (for UI display only)
NEXT_PUBLIC_ADMIN_WALLET=your_admin_wallet_address

# Automated Payouts (required for payout cron job)
FUNDING_WALLET_PRIVATE_KEY=your_private_key_base58
HELIUS_API_KEY=your_helius_api_key
CRON_SECRET=your_random_secure_string

# Solana network: 'devnet' or 'mainnet-beta'
SOLANA_NETWORK=devnet
